{
    "hosting": {
        "headers": [
            {
                "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
                "headers": [
                    {
                        "key": "Access-Control-Allow-Origin",
                        "value": "*"
                    }
                ]
            }
        ],
        "source": ".",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "frameworksBackend": {
            "region": "asia-east1"
        }
    }
}
